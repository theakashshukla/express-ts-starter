{
  "name": "@astreak/express-ts-starter",
  "version": "1.5.0",
  "main": "src/app.ts",
  "types": "src/app.ts", 
  "files": [
    "src",      
    "test",   
    ".env.example",
    ".gitignore",
    ".prettierrc.json",
    "Dockerfile",
    "docker-compose.yml",     
    "jest.config.js",  
    "tsconfig.json"     
  ],
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "start": "node --env-file=.env.production dist/app.js",
    "format": "prettier --write src/",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/theakashshukla/express-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/theakashshukla/express-ts-starter/issues"
  },
  "keywords": [
    "express",
    "typescript",
    "starter",
    "boilerplate",
    "authentication"
  ],
  "author": "Akash Shukla",
  "license": "ISC",
  "description": "Namskaram, this is a starter template for Express.js with TypeScript. It includes all the necessary packages and configurations to get started with your project. It also includes a basic user authentication system with JWT.",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.4",
    "morgan": "^1.10.0",
    "redis": "^4.6.14"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.10",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
